firewall {
    family inet {
        filter ctf-from-internet {
            term block_appliance {
                from {
                    destination-address {
                        10.128.4.8/29;
                    }
                }
                then {
                    discard;
                }
            }
            term allow_everything_else {
                then accept;
            }
        }
        filter ctf-to-internet {
            term block_appliance {
                from {
                    source-address {
                        10.128.4.8/29;
                    }
                }
                then {
                    discard;
                }
            }
            term allow_everything_else {
                then accept;
            }
        }
    }
    family inet6 {
        filter ctf-from-internet6 {
            term block_appliance {
                from {
                    destination-address {
                        2001:470:f026:504::8/125;
                    }
                }
                then discard;
            }
            term allow_everything_else {
                then accept;
            }
        }
        filter ctf-to-internet6 {
            term block_appliance {
                from {
                    source-address {
                        2001:470:f026:504::8/125;
                    }
                }
                then discard;
            }
            term allow_everything_else {
                then accept;
            }
        }
    }
}
